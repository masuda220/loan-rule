<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="example.datasource.loan.LoanMapper">
  <select id="history" resultType="example.domain.model.loan.Loan">
    SELECT
      蔵書番号 AS "蔵書番号.value",
      会員番号 AS "会員番号.value",
      貸出日 AS "貸出日"
    FROM 貸出.貸出
    WHERE
      会員番号 = #{value}
  </select>
</mapper>